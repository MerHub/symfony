    <script>

        jQuery( document ).ready(function( $ ) {
            $(window).resize(function(){

            })
            function maPosition(position) {
                var c="{{ path("updateGPS",{'lat':'var1','lon':'var2'}) }}";
                var finalPath = c.replace("var1", position.coords.latitude);
                var finalPath2 = finalPath.replace("var2", position.coords.longitude);
                $.ajax({
                    type: "post",
                    url: finalPath2,
                    contentType: "html",
                    success: function(response) {
                        //var htmlToDisplay = response.trim();
                        //$("#your_div").html(response);
                    }
                });
            }

            if(navigator.geolocation)
                navigator.geolocation.getCurrentPosition(maPosition);
        });


    </script>

    <!-- General section box modal start -->
    <div class="modal" id="section-settings" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog animated bounceInDown">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Section Settings</h4>
                </div>
                <div class="modal-body">

                    Body goes here...

                </div>
                <div class="modal-footer">
                    <button data-dismiss="modal" class="btn btn-default" type="button">Close</button>
                    <button class="btn btn-success" type="button">Save changes</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- modal end -->
