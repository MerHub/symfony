<script>
    Notification.requestPermission( function(status) {

    });

    $(function(){
        $(".markAll").click(function(){
            var y=confirm("voulez vous tout marquer comme lue");
            if(y==true){
                var c="{{ path("markAllRead") }}";
                $.ajax({
                    type: "post",
                    url: c,
                    contentType: "html",
                    success: function(response) {
                        if(response=="ok"){
                            $(".dropdown-menu-list.list-unstyled.ps-scrollbar").html("")
                            $(".notify-toggle-wrapper.list-inline-item.showopacity .badge.badge-pill.badge-accent").text(0)
                        }
                    }
                });
            }
        })
        var id_user={{app.user.id}}
        var encore=false;
        setInterval(function () {
            if(encore==false){
                var pr=parseInt($(".pace-progress").attr("data-progress"))
                if(pr>=99){
                    encore=true;
                    maFonctionDeTest();
                }
            }

        },200)
        $("body").on( "load",  function(){
            alert("oui")
        });
        $(".debut").fadeIn()

        function maFonctionDeTest(){

            /**
             * After calling notifications_assets() "pusher" is now available
             * and you can use it this way
            */

                // select the channel you want to listen to
            var channel = pusher.subscribe("notifications");// notifications channel
            channel.bind("my-event", function(data) {
             if(id_user==data.parameters.idReceive){

                 var n = new Notification(data.title, {body: data.description,icon: '{{ asset("font/images/logo.png") }}'}); // this also shows the notification
               var typ="";
                 var ic="";
               if(data.icon=="notif"){
                    typ="offline";
                    ic="fa fa-user";
               }
                 if(data.icon=="add"){
                     typ="available";
                     ic="fa fa-check";
                 }
                 if(data.icon=="delete"){
                     typ="busy";
                     ic="fa fa-times";
                 }
                 if(data.icon=="update"){
                     typ="unread away";
                     ic="fa fa-edit";
                 }
                var html='<li class="'+typ+'">';

                 html+=' <!-- available: success, warning, info, error -->\n' +
                     '                                        <a href="javascript:;">\n' +
                     '                                            <div class="notice-icon">\n' +
                     '                                                <i class="'+ic+'"></i>\n' +
                     '                                            </div>\n' +
                     '                                            <div>\n' +
                     '                                                    <span class="name">\n' +
                     '                                                        <strong>'+data.title+'</strong>\n' +
                     '                                                        <span class="time small">'+data.description+'</span>\n' +
                     '                                                    </span>\n' +
                     '                                            </div>\n' +
                     '                                        </a>\n' +
                     '                                    </li>';
                 $(".dropdown-menu-list.list-unstyled.ps-scrollbar").prepend(html)
                 $(".notify-toggle-wrapper.list-inline-item.showopacity .badge.badge-pill.badge-accent").text(parseInt($(".notify-toggle-wrapper.list-inline-item.showopacity .badge.badge-pill.badge-accent").text())+1)

             }
              //  console.log(data["test-message"]);
            });
            $(".preloader").fadeOut(500)
        }
    })
</script>
